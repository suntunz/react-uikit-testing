{
	"name": "@bitkub-moonshot/uikit-sdk",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": "14"
	},
	"repository": {
		"url": "https://gitlab.com/bitkub-blockchain/framework/uikit-sdk",
		"type": "git"
	},
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"prebuild": "rm -rf ./dist",
		"build": "tsc && yarn build-style",
		"build-style": "postcss ./src/styles/tailwind.css -o ./dist/style.css ",
		"build-storybook": "build-storybook --quiet",
		"build-storybook-style": "postcss ./src/styles/tailwind.css -o ./.storybook/tailwind.css",
		"dev": "build-storybook-style && start-storybook -p 6006 -s public",
		"storybook": "yarn build-storybook-style && start-storybook -p 6006",
		"eslint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"tailwind-config-viewer": "tailwind-config-viewer -o",
		"test": "jest"
	},
	"dependencies": {
		"babel-loader": "^8.2.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-react": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@storybook/addon-actions": "^6.2.1",
		"@storybook/addon-essentials": "^6.2.1",
		"@storybook/addon-links": "^6.2.1",
		"@storybook/node-logger": "^6.2.1",
		"@storybook/preset-create-react-app": "^3.1.7",
		"@storybook/react": "^6.2.1",
		"@types/jest": "^27.0.3",
		"@types/react": "^17.0.26",
		"@types/react-test-renderer": "^17.0.1",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"autoprefixer": "^10.4.0",
		"babel-jest": "^27.4.5",
		"eslint": "8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-react": "^7.25.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-tailwindcss": "^1.17.2",
		"jest": "^27.4.5",
		"postcss": "^8.4.5",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.0.2",
		"prettier": "^2.4.1",
		"react-test-renderer": "^17.0.2",
		"storybook-addon-designs": "^5.4.5",
		"storybook-tailwind-dark-mode": "^1.0.11",
		"tailwind-config-viewer": "^1.6.3",
		"tailwindcss": "^3.0.7",
		"typescript": "4.4.2"
	},
	"pre-commit": [
		"eslint"
	]
}
